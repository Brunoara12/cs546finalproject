<div class="container-fluid">
    <div class="row">
        {{> profilenav}}

        <div class="col-10">
            <div class="container">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">Date</th>
                            <th scope="col">Location</th>
                            <th scope="col">Type</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each activities}}
                            <tr>
                                <td>{{this.date}}</td>
                                <td>{{this.location}}</td>
                                <td>{{this.activity.0.name}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="/public/js/addactivity.js"></script>